# APIForge Configuration Example
# Copy this file to .env and update with your values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.1

# Custom LLM API Configuration (Optional)
# CUSTOM_API_KEY=your_custom_api_key
# CUSTOM_BASE_URL=https://your-custom-api.com/v1
# CUSTOM_MODEL=your-custom-model

# Qwen API Configuration
QWEN_BASE_URL=http://your-qwen-server:port/v1
QWEN_MODEL=Qwen3-32B

# LLM Provider Settings
LLM_PROVIDER=openai  # Options: openai, custom, qwen

# Logging Configuration
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FORMAT=structured  # Options: structured, simple

# Performance Settings
MAX_CONCURRENT_REQUESTS=10
RATE_LIMIT_PER_MINUTE=100

# Execution Mode
EXECUTION_MODE=auto  # Options: auto, fast, smart, ai-analysis
ENABLE_INTELLIGENT_SCHEDULING=false

# Security Settings
SECURE_MODE=true
SANITIZE_LOGS=true